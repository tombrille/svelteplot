{
    "name": "svelteplot",
    "version": "0.2.0",
    "license": "ISC",
    "author": {
        "name": "Gregor Aisch",
        "email": "gka@users.noreply.github.com"
    },
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "npm run test:unit",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check src && eslint src",
        "format": "prettier --write .",
        "test:unit": "vitest",
        "prepack": "npx svelte-package",
        "release-next": "npm version prerelease --preid next && npm publish && git push && git push --tags && sleep 1 && npm dist-tag add svelteplot@$(npm view . version) next",
        "docs": "npm run build && cd build && rsync --recursive . vis4.net:svelteplot/alpha0/"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        },
        "./*.js": {
            "import": "./dist/*.js"
        },
        "./*.svelte": {
            "import": "./dist/*.svelte"
        },
        "./core/*.svelte": {
            "import": "./dist/core/*.svelte"
        },
        "./marks/*.svelte": {
            "import": "./dist/marks/*.svelte"
        }
    },
    "main": "./dist/index.js",
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "devDependencies": {
        "@aitodotai/json-stringify-pretty-compact": "^1.3.0",
        "@emotion/css": "^11.13.5",
        "@sveltejs/adapter-auto": "^6.0.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/eslint-config": "^8.2.0",
        "@sveltejs/kit": "^2.21.0",
        "@sveltejs/package": "^2.3.11",
        "@sveltejs/vite-plugin-svelte": "5.0.3",
        "@sveltepress/theme-default": "^6.0.3",
        "@sveltepress/twoslash": "^1.2.2",
        "@sveltepress/vite": "^1.2.2",
        "@testing-library/svelte": "^5.2.7",
        "@testing-library/user-event": "^14.6.1",
        "@types/d3-array": "^3.2.1",
        "@types/d3-color": "^3.1.3",
        "@types/d3-dsv": "^3.0.7",
        "@types/d3-geo": "^3.1.0",
        "@types/d3-interpolate": "^3.0.4",
        "@types/d3-path": "^3.1.1",
        "@types/d3-random": "^3.0.3",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-scale-chromatic": "^3.1.0",
        "@types/d3-shape": "^3.1.7",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "csstype": "^3.1.3",
        "d3-dsv": "^3.0.1",
        "d3-fetch": "^3.0.1",
        "d3-force": "^3.0.0",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-svelte": "3.7.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "remark-code-extra": "^1.0.1",
        "remark-code-frontmatter": "^1.0.0",
        "resize-observer-polyfill": "^1.5.1",
        "sass": "^1.89.0",
        "svelte-check": "^4.2.1",
        "svelte-eslint-parser": "1.2.0",
        "svelte-highlight": "^7.8.3",
        "topojson-client": "^3.1.0",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.4",
        "typedoc-plugin-markdown": "^4.6.3",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vitest": "^3.1.3",
        "vitest-matchmedia-mock": "^2.0.3"
    },
    "types": "./dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "d3-array": "^3.2.4",
        "d3-color": "^3.1.0",
        "d3-format": "^3.1.0",
        "d3-geo": "^3.1.1",
        "d3-interpolate": "^3.0.1",
        "d3-path": "^3.1.0",
        "d3-quadtree": "^3.0.1",
        "d3-random": "^3.0.1",
        "d3-regression": "^1.3.10",
        "d3-scale": "^4.0.2",
        "d3-scale-chromatic": "^3.1.0",
        "d3-shape": "^3.2.0",
        "d3-time": "^3.1.0",
        "es-toolkit": "^1.37.2",
        "fast-equals": "^5.2.2",
        "merge-deep": "^3.0.3",
        "svelte": "5.30.1"
    }
}
